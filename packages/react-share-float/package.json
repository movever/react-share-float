{
  "name": "react-share-float",
  "version": "0.1.0",
  "description": "Floating share component based on react-share.",
  "type": "module",
  "main": "dist/react-share-float.cjs",
  "module": "dist/react-share-float.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-share-float.es.js",
      "require": "./dist/react-share-float.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "dependencies": {
    "react-share": "^5.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "@vitejs/plugin-react": "^4.3.1"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && vite build",
    "dev": "vite",
    "clean": "rimraf dist"
  }
}


