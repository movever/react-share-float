{
  "name": "react-share-float",
  "version": "0.1.0",
  "description": "Floating share component based on react-share with auto-injected styles.",
  "type": "module",
  "main": "dist/react-share-float.cjs",
  "module": "dist/react-share-float.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-share-float.es.js",
      "require": "./dist/react-share-float.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "vite build",
    "dev": "npm --prefix examples/basic run dev",
    "preview": "npm --prefix examples/basic run preview",
    "clean": "git clean -fdX"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "clsx": "^2.1.1",
    "react-share": "^5.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "vite": "^5.4.0"
  }
}
